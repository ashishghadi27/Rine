<!-- <?xml version="1.0" encoding="utf-8"?> -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="5dip" android:color="#808080" />
            <gradient android:angle="0" android:startColor="#808080" android:endColor="#808080"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="5dip" android:color="#808080" />
            <solid android:color="#808080"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="5dip" android:color="#d3d3d3" />
            <gradient android:angle="0" android:startColor="#d3d3d3" android:endColor="#d3d3d3" />
        </shape>
    </item>
</selector>